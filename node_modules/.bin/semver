#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/Greg/Projects/apps/zyp/zyp-community/workspace/zyp-platform/apps/hello-world/node_modules/semver/bin/node_modules:/Users/Greg/Projects/apps/zyp/zyp-community/workspace/zyp-platform/apps/hello-world/node_modules/semver/node_modules:/Users/Greg/Projects/apps/zyp/zyp-community/workspace/zyp-platform/apps/hello-world/node_modules:/Users/Greg/Projects/apps/zyp/zyp-community/workspace/zyp-platform/apps/node_modules:/Users/Greg/Projects/apps/zyp/zyp-community/workspace/zyp-platform/node_modules:/Users/Greg/Projects/apps/zyp/zyp-community/workspace/node_modules:/Users/Greg/Projects/apps/zyp/zyp-community/node_modules:/Users/Greg/Projects/apps/zyp/node_modules:/Users/Greg/Projects/apps/node_modules:/Users/Greg/Projects/node_modules:/Users/Greg/node_modules:/Users/node_modules:/node_modules:/Users/Greg/Projects/apps/zyp/zyp-community/workspace/zyp-platform/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/Greg/Projects/apps/zyp/zyp-community/workspace/zyp-platform/apps/hello-world/node_modules/semver/bin/node_modules:/Users/Greg/Projects/apps/zyp/zyp-community/workspace/zyp-platform/apps/hello-world/node_modules/semver/node_modules:/Users/Greg/Projects/apps/zyp/zyp-community/workspace/zyp-platform/apps/hello-world/node_modules:/Users/Greg/Projects/apps/zyp/zyp-community/workspace/zyp-platform/apps/node_modules:/Users/Greg/Projects/apps/zyp/zyp-community/workspace/zyp-platform/node_modules:/Users/Greg/Projects/apps/zyp/zyp-community/workspace/node_modules:/Users/Greg/Projects/apps/zyp/zyp-community/node_modules:/Users/Greg/Projects/apps/zyp/node_modules:/Users/Greg/Projects/apps/node_modules:/Users/Greg/Projects/node_modules:/Users/Greg/node_modules:/Users/node_modules:/node_modules:/Users/Greg/Projects/apps/zyp/zyp-community/workspace/zyp-platform/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../semver/bin/semver.js" "$@"
else
  exec node  "$basedir/../semver/bin/semver.js" "$@"
fi
