#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/e36e0a56-30b2-48a7-986d-8da43d9839da/node_modules/baseline-browser-mapping/dist/node_modules:/workspaces/e36e0a56-30b2-48a7-986d-8da43d9839da/node_modules/baseline-browser-mapping/node_modules:/workspaces/e36e0a56-30b2-48a7-986d-8da43d9839da/node_modules:/workspaces/node_modules:/node_modules:/workspaces/e36e0a56-30b2-48a7-986d-8da43d9839da/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/e36e0a56-30b2-48a7-986d-8da43d9839da/node_modules/baseline-browser-mapping/dist/node_modules:/workspaces/e36e0a56-30b2-48a7-986d-8da43d9839da/node_modules/baseline-browser-mapping/node_modules:/workspaces/e36e0a56-30b2-48a7-986d-8da43d9839da/node_modules:/workspaces/node_modules:/node_modules:/workspaces/e36e0a56-30b2-48a7-986d-8da43d9839da/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../baseline-browser-mapping/dist/cli.js" "$@"
else
  exec node  "$basedir/../baseline-browser-mapping/dist/cli.js" "$@"
fi
