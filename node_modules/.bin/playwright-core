#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/de2d19f3-8c1d-4c30-9740-e68213e02234/node_modules/playwright-core/node_modules:/workspaces/de2d19f3-8c1d-4c30-9740-e68213e02234/node_modules:/workspaces/node_modules:/node_modules:/workspaces/de2d19f3-8c1d-4c30-9740-e68213e02234/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/de2d19f3-8c1d-4c30-9740-e68213e02234/node_modules/playwright-core/node_modules:/workspaces/de2d19f3-8c1d-4c30-9740-e68213e02234/node_modules:/workspaces/node_modules:/node_modules:/workspaces/de2d19f3-8c1d-4c30-9740-e68213e02234/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../playwright-core/cli.js" "$@"
else
  exec node  "$basedir/../playwright-core/cli.js" "$@"
fi
