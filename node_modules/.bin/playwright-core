#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/7f660eb8-cb86-4eaa-b770-93036d9f94ed/node_modules/playwright-core/node_modules:/workspaces/7f660eb8-cb86-4eaa-b770-93036d9f94ed/node_modules:/workspaces/node_modules:/node_modules:/workspaces/7f660eb8-cb86-4eaa-b770-93036d9f94ed/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/7f660eb8-cb86-4eaa-b770-93036d9f94ed/node_modules/playwright-core/node_modules:/workspaces/7f660eb8-cb86-4eaa-b770-93036d9f94ed/node_modules:/workspaces/node_modules:/node_modules:/workspaces/7f660eb8-cb86-4eaa-b770-93036d9f94ed/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../playwright-core/cli.js" "$@"
else
  exec node  "$basedir/../playwright-core/cli.js" "$@"
fi
