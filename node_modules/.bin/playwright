#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/e36e0a56-30b2-48a7-986d-8da43d9839da/node_modules/.pnpm/@playwright+test@1.49.1/node_modules/@playwright/test/node_modules:/workspaces/e36e0a56-30b2-48a7-986d-8da43d9839da/node_modules/.pnpm/@playwright+test@1.49.1/node_modules/@playwright/node_modules:/workspaces/e36e0a56-30b2-48a7-986d-8da43d9839da/node_modules/.pnpm/@playwright+test@1.49.1/node_modules:/workspaces/e36e0a56-30b2-48a7-986d-8da43d9839da/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/e36e0a56-30b2-48a7-986d-8da43d9839da/node_modules/.pnpm/@playwright+test@1.49.1/node_modules/@playwright/test/node_modules:/workspaces/e36e0a56-30b2-48a7-986d-8da43d9839da/node_modules/.pnpm/@playwright+test@1.49.1/node_modules/@playwright/node_modules:/workspaces/e36e0a56-30b2-48a7-986d-8da43d9839da/node_modules/.pnpm/@playwright+test@1.49.1/node_modules:/workspaces/e36e0a56-30b2-48a7-986d-8da43d9839da/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@playwright/test/cli.js" "$@"
else
  exec node  "$basedir/../@playwright/test/cli.js" "$@"
fi
